{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": ["yii2", "framework", "basic", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "ext-json":  "*",
        "ext-fileinfo": "*",
        "ext-xml": "*",
        "yiisoft/yii2": ">=2.0.14",
        "yiisoft/yii2-swiftmailer": ">=2.1.0",
        "bariew/yii2-node-tree" : "dev-master",
        "creocoder/yii2-nested-sets": "0.9.*",
        "yiisoft/yii2-jui": "*",
        "yiisoft/yii2-authclient": ">=2.0.0",
        "kartik-v/yii2-widget-select2": ">=2.1@dev",
        "kartik-v/yii2-editors": "@dev",
        "kartik-v/yii2-widget-fileinput": "^1.0@dev",
        "yiisoft/yii2-bootstrap4": "^1.0@dev",
        "sweelix/yii2-postmark": "dev-master",
        "kartik-v/yii2-datecontrol": "1.9.x-dev",
        "kartik-v/yii2-widget-datetimepicker": "1.4.x-dev",
        "kartik-v/yii2-number": "1.0.x-dev",
        "kartik-v/yii2-date-range": "1.7.x-dev",
        "rubix/ml": "dev-master",
        "scheb/yahoo-finance-api": "4.x-dev"
    },
    "require-dev": {
        "yiisoft/yii2-debug": ">=2.1.0",
        "yiisoft/yii2-gii": ">=2.1.0",
        "yiisoft/yii2-faker": ">=2.0.0",

        "codeception/base": ">=2.3.0",
        "codeception/verify": ">=0.4.0",
        "codeception/specify": ">=0.4.6",
        "symfony/browser-kit": ">=2.7"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "app/config/local.php"
            ]
        }
    },
    "repositories": [
        {
            "packagist": false
        },
        {
            "type": "composer",
            "url": "https://packagist.org/"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
